{
  "manifest_version": 3,
  "name": "GraphQL Inspector",
  "version": "1.0",
  "description": "A Chrome extension to identify and highlight GraphQL requests in the network tab",
  "permissions": [
    "activeTab",
    "storage",
    "webRequest",
    "debugger"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "GraphQL Inspector"
  },
  "devtools_page": "devtools.html",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
